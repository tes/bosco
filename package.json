{
  "name": "bosco",
  "version": "4.1.1",
  "description": "Bosco will take care of your microservices, just don't try and use him on a plane.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "nyc mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm audit"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "micro",
    "service",
    "build",
    "automation",
    "minification",
    "s3",
    "project"
  ],
  "bin": {
    "bosco": "./bin/bosco.js"
  },
  "man": [
    "./man/bosco-activity.3",
    "./man/bosco-cdn.3",
    "./man/bosco-clean.3",
    "./man/bosco-clone.3",
    "./man/bosco-commit.3",
    "./man/bosco-config.3",
    "./man/bosco-exec.3",
    "./man/bosco-grep.3",
    "./man/bosco-install.3",
    "./man/bosco-morning.3",
    "./man/bosco-ps.3",
    "./man/bosco-pull-docker.3",
    "./man/bosco-pull-git.3",
    "./man/bosco-pull.3",
    "./man/bosco-pushall.3",
    "./man/bosco-run.3",
    "./man/bosco-s3delete.3",
    "./man/bosco-s3push.3",
    "./man/bosco-setup.3",
    "./man/bosco-stop.3",
    "./man/bosco-team.3",
    "./man/bosco-template.3",
    "./man/bosco-upstream.3"
  ],
  "directories": {
    "bin": "./bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tes/bosco.git"
  },
  "author": "clifton.cunningham@tes.co.uk",
  "license": "MIT",
  "dependencies": {
    "async": "^1.4.2",
    "bluebird": "^3.7.2",
    "bosco-core": "^0.2.0",
    "clean-css": "^4.2.1",
    "cli-table": "^0.3.1",
    "colors": "^1.4.0",
    "dockerode": "^3.0.2",
    "figlet": "^1.1.1",
    "fs-extra": "^8.1.0",
    "glob": "^7.0.3",
    "handlebars": "^4.7.2",
    "iltorb": "^2.3.2",
    "inquirer": "^1.2.3",
    "json2csv": "^3.7.3",
    "lodash": "^4.17.15",
    "mime": "^1.2.11",
    "moment": "^2.12.0",
    "octonode": "^0.9.5",
    "parse-link-header": "^0.4.1",
    "pm2": "^4.2.3",
    "prettyjson": "^1.2.1",
    "request": "^2.40.0",
    "semver": "^5.0.3",
    "sf": "^0.1.7",
    "symlink": "^2.0.0",
    "tail": "^1.1.0",
    "tar-fs": "^1.2.0",
    "traverse": "^0.6.6",
    "treeify": "^1.0.1",
    "tty-table": "^2.7.3",
    "uglify-js": "^2.6.1",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.9",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "expect.js": "^0.3.1",
    "gulp": "^4.0.2",
    "husky": "^1.2.1",
    "marked": "^0.7.0",
    "marked-man": "^0.6.0",
    "mocha": "^5.2.0",
    "nyc": "^15.0.0",
    "sinon": "^1.17.1"
