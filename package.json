{
  "name": "bosco",
  "version": "0.5.57",
  "description": "Bosco will take care of your microservices, just don't try and use him on a plane.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "f(){ if [ \"$#\" -gt 0 ]; then npm run testonly -- \"$@\"; else npm run check-coverage; fi; };f",
    "testonly": "mocha",
    "coverage": "istanbul cover --include-all-sources true _mocha",
    "check-coverage": "npm run coverage && istanbul check-coverage --statements 16",
    "coveralls": "npm run coverage && npm run coveralls-after-test",
    "coveralls-after-test": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "precommit": "npm run lint && npm run test"
  },
  "keywords": [
    "micro",
    "service",
    "build",
    "automation",
    "minification",
    "s3",
    "project"
  ],
  "bin": {
    "bosco": "./bin/bosco.js"
  },
  "man": [
    "./man/bosco-activity.3",
    "./man/bosco-audit.3",
    "./man/bosco-cdn.3",
    "./man/bosco-clean.3",
    "./man/bosco-clone.3",
    "./man/bosco-commit.3",
    "./man/bosco-config.3",
    "./man/bosco-exec.3",
    "./man/bosco-grep.3",
    "./man/bosco-install.3",
    "./man/bosco-morning.3",
    "./man/bosco-ps.3",
    "./man/bosco-pull-docker.3",
    "./man/bosco-pull-git.3",
    "./man/bosco-pull.3",
    "./man/bosco-pushall.3",
    "./man/bosco-run.3",
    "./man/bosco-s3delete.3",
    "./man/bosco-s3push.3",
    "./man/bosco-setup.3",
    "./man/bosco-stop.3",
    "./man/bosco-team.3",
    "./man/bosco-template.3",
    "./man/bosco-upstream.3"
  ],
  "directories": {
    "bin": "./bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tes/bosco.git"
  },
  "author": "clifton.cunningham@tes.co.uk",
  "license": "MIT",
  "dependencies": {
    "appdirectory": "^0.1.0",
    "asciify": "^1.3.5",
    "async": "^1.4.2",
    "browser-sync": "^2.11.2",
    "clean-css": "^3.1.9",
    "cli-table": "^0.3.0",
    "colors": "^1.0.3",
    "dockerode": "^2.2.10",
    "figlet": "^1.1.1",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.3",
    "handlebars": "^4.0.3",
    "highland": "^2.2.0",
    "inquirer": "^1.1.2",
    "knox": "^0.9.0",
    "lodash": "^4.13.1",
    "mime": "^1.2.11",
    "moment": "^2.12.0",
    "nconf": "^0.8.1",
    "nsp": "^2.0.2",
    "octonode": "^0.7.4",
    "osenv": "^0.1.0",
    "parse-link-header": "^0.4.1",
    "pm2": "^2.0.5",
    "prettyjson": "^1.0.0",
    "progress": "^1.1.8",
    "prompt": "^1.0.0",
    "request": "^2.40.0",
    "semver": "^5.0.3",
    "sf": "^0.1.7",
    "symlink": "^2.0.0",
    "tail": "^1.1.0",
    "tar-fs": "^1.2.0",
    "treeify": "^1.0.1",
    "uglify-js": "^2.6.1",
    "yargs": "^4.8.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.1",
    "eslint": "^1.10.2",
    "eslint-config-airbnb": "^0.1.0",
    "expect.js": "^0.3.1",
    "gulp": "^3.8.7",
    "gulp-marked-man": "^0.3.1",
    "husky": "^0.11.5",
    "istanbul": "^0.3.2",
    "mocha": "^2.2.1",
    "sinon": "^1.17.1"
  },
  "config": {
    "blanket": {
      "pattern": "src"
    }
  }
}
