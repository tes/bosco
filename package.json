{
  "name": "bosco",
  "version": "0.4.1",
  "description": "Bosco will take care of your microservices, just don't try and use him on a plane.",
  "main": "index.js",
  "scripts": {
    "lint": "jshint .",
    "test": "istanbul cover _mocha -- -R spec tests/* && istanbul check-coverage --statements 80",
    "coveralls": "NODE_ENV=test mocha --require blanket --reporter mocha-lcov-reporter tests/* | ./node_modules/coveralls/bin/coveralls.js"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "keywords": [
    "micro",
    "service",
    "build",
    "automation",
    "minification",
    "s3",
    "project"
  ],
  "bin": {
    "bosco": "./bin/bosco.js"
  },
  "directories": {
    "man": "./man",
    "bin": "./bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tes/bosco.git"
  },
  "author": "clifton.cunningham@tes.co.uk",
  "license": "MIT",
  "dependencies": {
    "asciify": "^1.3.5",
    "async": "^0.9.0",
    "clean-css": "^2.2.13",
    "cli-table": "^0.3.0",
    "colors": "^1.0.3",
    "commander": "^2.3.0",
    "dateformat": "^1.0.8-1.2.3",
    "diff": "^1.0.8",
    "dockerode": "^2.0.3",
    "event-stream": "^3.1.7",
    "figlet": "~1.1.0",
    "gulp": "^3.8.7",
    "gulp-git": "^0.5.0",
    "gulp-mocha": "^2.0.0",
    "handlebars": "^2.0.0-alpha.4",
    "highland": "^2.2.0",
    "inquirer": "^0.8.0",
    "knox": "^0.9.0",
    "lodash": "^2.4.1",
    "lodash-node": "^2.4.1",
    "mime": "^1.2.11",
    "mkdirp": "^0.5.0",
    "moment": "^2.8.3",
    "multimeter": "^0.1.1",
    "nconf": "^0.7.1",
    "node-sass": "^2.0.1",
    "nplugm": "^1.0.1",
    "nsp": "^0.4.2",
    "octonode": "^0.6.13",
    "osenv": "^0.1.0",
    "parse-link-header": "^0.2.0",
    "pm2": "^0.12.1",
    "prettyjson": "^1.0.0",
    "progress": "^1.1.8",
    "prompt": "^0.2.13",
    "request": "^2.40.0",
    "semver": "^4.1.1",
    "sf": "^0.1.7",
    "tail": "^0.4.0",
    "tar-fs": "^1.2.0",
    "uglify-js": "^2.4.15",
    "watch": "^0.13.0"
  },
  "devDependencies": {
    "assert": "^1.1.1",
    "blanket": "^1.1.6",
    "coveralls": "^2.11.1",
    "expect.js": "^0.3.1",
    "gulp": "^3.8.8",
    "gulp-marked-man": "^0.3.1",
    "gulp-util": "^3.0.1",
    "istanbul": "^0.3.2",
    "jshint": "^2.5.6",
    "mocha": "^1.21.4",
    "mocha-lcov-reporter": "0.0.1",
    "precommit-hook": "^1.0.7",
    "through2": "^0.6.2"
  },
  "config": {
    "blanket": {
      "pattern": "src"
    }
  }
}
